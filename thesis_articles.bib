@article{allenECOLOGYMYCORRHIZAEConceptual2003,
  title = {{{ECOLOGY OF MYCORRHIZAE}}: {{A Conceptual Framework}} for {{Complex Interactions Among Plants}} and {{Fungi}}},
  shorttitle = {{{ECOLOGY OF MYCORRHIZAE}}},
  author = {Allen, M.f. and Swenson, W. and Querejeta, J.i. and Egerton-Warburton, L.m. and Treseder, K.k.},
