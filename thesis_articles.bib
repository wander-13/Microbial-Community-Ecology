  date = {2021-09},
  journaltitle = {The ISME Journal},
  shortjournal = {ISME J},
  volume = {15},
  number = {9},
  pages = {2692--2707},
  publisher = {{Nature Publishing Group}},
  issn = {1751-7370},
  doi = {10.1038/s41396-021-00944-8},
